{
  "name": "ap-wimhof",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "lint": "standard --verbose | snazzy",
    "start": "concurrently --kill-others \"npm run start:srv\" \"npm run start:ui:dev\"",
    "start:ui:dev": "webpack serve --config webpack.config.dev.js --progress",
    "start:srv": "ts-node ./src-serv/index",
    "test": "jest",
    "test-coverage": "jest --collectCoverage",
    "test-update": "jest --updateSnapshot"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "latest",
    "@fortawesome/free-regular-svg-icons": "latest",
    "@fortawesome/free-solid-svg-icons": "latest",
    "@fortawesome/react-fontawesome": "latest",
    "@reduxjs/toolkit": "latest",
    "bootstrap": "latest",
    "get-audio-duration": "^3.1.0",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "i18next-http-backend": "latest",
    "immer": "latest",
    "jquery": "latest",
    "node-sass": "latest",
    "popper.js": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "latest",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "redux": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@types/express": "latest",
    "autoprefixer": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "babel-loader": "latest",
    "clean-webpack-plugin": "latest",
    "concurrently": "latest",
    "copy-webpack-plugin": "latest",
    "css-loader": "latest",
    "express": "latest",
    "file-loader": "latest",
    "html-webpack-plugin": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "postcss-loader": "latest",
    "react-test-renderer": "latest",
    "sass-loader": "latest",
    "snazzy": "latest",
    "standard": "latest",
    "style-loader": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "webpack-merge": "latest"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "node": true,
      "jest": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
